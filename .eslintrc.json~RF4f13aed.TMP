{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
  	"linebreak-style": "off",
  	"class-methods-use-this": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/prefer-stateless-function" : "warn"
  }
}

