{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "linebreak-style": "off",
    "class-methods-use-this": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/prefer-stateless-function": "warn",
    "comma-dangle": "off",
    "react/prop-types": "off",
    "react/destructuring-assignment": "off"
  }
}

